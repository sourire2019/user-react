{
  "name": "ice-koa-react-scaffold",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "client": "ice dev --project-type=node",
    "server": "nodemon server/app.js",
    "start": "npm run client & npm run server",
    "build": "ice build --project-type=node",
    "lint": "eslint . --ext '.js,.jsx' --fix"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com",
    "access": "public"
  },
  "buildConfig": {
    "theme": "@icedesign/skin",
    "entry": "client/index.js",
    "localization": false
  },
  "proxyConfig": {
    "/api": {
      "enable": true,
      "target": "http://127.0.0.1:3000"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@antv/data-set": "^0.8.7",
    "@icedesign/base": "^0.2.0",
    "@icedesign/container": "^0.1.2",
    "@icedesign/form-binder": "^1.0.3",
    "@icedesign/icon": "^0.1.1",
    "@icedesign/img": "^0.1.1",
    "@icedesign/layout": "^0.1.2",
    "@icedesign/menu": "^0.1.1",
    "@icedesign/skin": "^0.1.0",
    "@koa/cors": "^2.2.2",
    "bizcharts": "^3.1.5",
    "enquire-js": "^0.1.2",
    "foundation-symbol": "^0.1.3",
    "js-file-download": "^0.4.4",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-respond": "^2.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.5.8",
    "qrcode.react": "^0.8.0",
    "react": "^16.4.1",
    "react-cookies": "^0.1.0",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "koa-logger": "^3.2.0",
    "nodemon": "^1.18.4",
    "ice-scripts": "^1.6.0-next.3"
  },
  "title": "user-react",
  "templateType": "Koa"
